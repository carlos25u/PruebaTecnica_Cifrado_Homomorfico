<Window x:Class="PruebaTecnica_Cifrado_Homomorfico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PruebaTecnica_Cifrado_Homomorfico"
        mc:Ignorable="d"
        Title="Menu principal" WindowStartupLocation="CenterScreen" Height="450" Width="800" Icon="/Resource/LOGO.png">
    <Grid>
        <StackPanel Orientation="Vertical">

            <Menu>
                <MenuItem Header="Registro">
                    <MenuItem Header="Clientes" x:Name="RegistroCliente" Click="RegistroCliente_Click"></MenuItem>
                </MenuItem>

                <MenuItem Header="Consultas">
                    <MenuItem Header="Clientes" x:Name="ConsultasClientes" Click="ConsultasClientes_Click"></MenuItem>
                </MenuItem>
            </Menu>
            <Image Source="/Resource/LOGO.png" Width="250" Margin="0,70,0,0"
                   HorizontalAlignment="Center" VerticalAlignment="Center"></Image>
            
        </StackPanel>
    </Grid>
</Window>
